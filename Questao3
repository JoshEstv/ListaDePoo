import java.util.LinkedList;
import java.util.Queue;

class PilhaFila {
    private Queue<Integer> fila1 = new LinkedList<>();
    private Queue<Integer> fila2 = new LinkedList<>();
    
    public void push(int elemento) {
        fila2.add(elemento);
        while (!fila1.isEmpty()) {
            fila2.add(fila1.poll());
        }
        Queue<Integer> temp = fila1;
        fila1 = fila2;
        fila2 = temp;
    }
    
    public int pop() {
        if (!fila1.isEmpty()) {
            return fila1.poll();
        }
        throw new RuntimeException("A pilha est√° vazia");
    }
}
